generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id          Int      @id @default(autoincrement())
  name        String?
  password    String
  email       String   @unique
  is_verified Boolean  @default(false)
  created_at  DateTime @default(now())
}
